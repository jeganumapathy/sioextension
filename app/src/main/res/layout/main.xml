<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/receiveText"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="create chat group" />

    <EditText
        android:id="@+id/edtGroup"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btnCreateGroup"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="create button" />

    <Button
        android:id="@+id/btnSendMessage"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Send Message" />

    <com.devilo.sioextension.test.MyView
        android:id="@+id/view"
        android:layout_width="100dp"
        android:layout_height="100dp"></com.devilo.sioextension.test.MyView>

    <com.devilo.sioextension.test.MyView
        android:id="@+id/view1"
        android:layout_width="100dp"
        android:layout_height="100dp"></com.devilo.sioextension.test.MyView>


</LinearLayout>